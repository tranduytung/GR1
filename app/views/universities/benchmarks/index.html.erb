<div class="box box-primary">
  <div class="box-body box-profile">
<h2 class="text-center"><%= t("university.manage_benchmarks") %></h2>
  <%= link_to t("university.export_benchmark"),
    export_file_excel_universities_benchmarks_path(format: 'xlsx') %>
  <%= form_tag import_file_excel_universities_benchmarks_path, class:'form-inline', multipart: true do %>
      <div class="form-group">
        <%= label_tag 'select_file_excel', nil, class: 'sr-only' %>
        <%= file_field_tag :file , class: 'btn btn-default' %>
      </div>
      <div class="form-group">
        <%= label_tag 'import_excel', nil, class: 'sr-only' %>
        <%= submit_tag t("import_excel") , class: 'btn btn-default' %>
      </div>
  <% end %>
</div>
</div>
<div class="box box-primary">
      <div class="box-body box-profile">
<table class="table table-bordered margin-0">
  <thead>
    <tr>
      <th class="center"><%= t "major.code" %></th>
      <th class="center"><%= t "major.name" %></th>
      <th class="center"><%= t "major.target" %></th>
      <th class="center"><%= t "major.group_exam" %></th>
      <th class="center"><%= t "major.total" %></th>
      <th class="center", colspan="4"><%= t "admin.actions" %></th>
    </tr>
  </thead>
  <tbody>
    <% @majors.each do |major| %>
      <%= render "major", major: major%>
    <% end %>
  </tbody>
</table>
</div>
</div>
