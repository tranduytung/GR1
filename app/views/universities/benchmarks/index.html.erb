<div class="box box-primary">
  <div class="box-body box-profile">
<h2 class="text-center">Benchmark#index</h2>
 <!--  <%= link_to "export_csv", export_file_excel_universities_majors_path(format: 'xlsx') %> -->
  <%= link_to "export_benchmark", export_file_excel_universities_benchmarks_path(format: 'xlsx') %>
  <%= form_tag import_file_excel_universities_benchmarks_path, class:'form-inline', multipart: true do %>
      <div class="form-group">
        <%= label_tag 'select_file_excel', nil, class: 'sr-only' %>
        <%= file_field_tag :file , class: 'btn btn-default' %>
      </div>
      <div class="form-group">
        <%= label_tag 'import_excel', nil, class: 'sr-only' %>
        <%= submit_tag "Import Excel for benchmark" , class: 'btn btn-default' %>
      </div>
  <% end %>
</div>
</div>
<div class="box box-primary">
      <div class="box-body box-profile">
<table class="table table-bordered margin-0">
  <thead>
    <tr>
      <th class="center"><%= t "major.code" %></th>
      <th class="center"><%= t "major_info.name" %></th>
      <th class="center"><%= t "major_info.target" %></th>
      <th class="center"><%= t "major_info.group_exams" %></th>
      <th class="center"><%= t "major_info.total" %></th>
      <th class="center", colspan="4"><%= t "admin.actions" %></th>
    </tr>
  </thead>
  <tbody>
    <% @majors.each do |major| %>
      <%= render "major", major: major%>
    <% end %>
  </tbody>
</table>
</div>
</div>
