<% provide :title, t("university.all") %>
<div class="row">
  <h2 class="text-center"><%= t "university.all" %></h2>
  <div class="col-md-4">
    <div class="box box-primary">
      <div class="box-body box-profile">
      Co tong cong: <%= University.count %> truong dai hoc

      </div>
    </div>
    <div class="box box-primary">
      <div class="box-body box-profile">
        <%= form_tag import_file_excel_admins_universities_path, class:'form-inline', multipart: true do %>
          <div class="form-group">
            <%= label_tag 'select_file_excel', nil, class: 'sr-only' %>
            <%= file_field_tag :file , class: 'btn btn-default' %>
          </div>
          <div class="form-group">
            <%= label_tag 'import_excel', nil, class: 'sr-only' %>
            <%= submit_tag "Import Excel" , class: 'btn btn-default' %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  <div  class="col-md-8">
    <div class="box box-primary">
      <div class="box-body box-profile">
        <table class="table table-bordered table-striped">
          <thead>
            <tr>
              <th class="center"><%= t "examinee.code" %></th>
              <th class="center"><%= t "examinee.name" %></th>
              <th class="center"><%= t "admin.actions" %></th>
            </tr>
          </thead>
          <tbody>
            <%= render @universities %>
          </tbody>
        </table>
        <div class="text-center">
          <%= paginate @universities %>
        </div>
      </div>
    </div>
  </div>
</div>
