<div class="row">
  <div class="col-md-6 text col-md-offset-3">
    <div class="box box-primary">
      <div class="box-body box-profile">
        <h3 class="text-center">
          <%= @university.name %>
        </h3>
        <h4>
        <div class="list-group list-group-unbordered">
          <div class="list-group-item">
            <span class="glyphicon glyphicon-user"></span>
            <%= t("university.code") %>: <%= @university.code %>
          </div>
          <div class="list-group-item">
            <span class="glyphicon glyphicon-envelope"></span>
            <%= t("university.email") %>: <%= @university.email %>
          </div>
          <div class="list-group-item">
            <span class="glyphicon glyphicon-earphone"></span>
            <%= t("university.phone") %>: <%= @university.phone %>
          </div>
          <div class="list-group-item">
            <span class="glyphicon glyphicon-asterisk"></span>
            <%= t("university.address") %>: <%= @university.address %>
          </div>
          <div class="list-group-item">
            <span class="glyphicon glyphicon-inbox"></span>
            <%= t("university.web_site") %>: <%= @university.web_site %>
          </div>
        </div>
        </h4>
      </div>
    </div>
  </div>
</div>
