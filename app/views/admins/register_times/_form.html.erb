<%= form_for [:admins, @register_time] do |f| %>
  <%= render "shared/error_messages", object: @register_time %>

  <%= f.number_field :year, value: Time.now.year, class: "form-control text-center", disabled: true %>
  <div class="col-md-4">
    <div class="col-md-6">
      <%= f.label :start_aspiration_1 %>
      <%= f.date_field :start_aspiration_1, class: "form-control" %>
    </div>
    <div class="col-md-6">
      <%= f.label :end_aspiration_1 %>
      <%= f.date_field :end_aspiration_1, class: "form-control" %>
    </div>
  </div>
  <div class="col-md-4">
    <div class="col-md-6">
      <%= f.label :start_aspiration_2 %>
      <%= f.date_field :start_aspiration_2, class: "form-control" %>
    </div>
    <div class="col-md-6">
      <%= f.label :end_aspiration_2 %>
      <%= f.date_field :end_aspiration_2, class: "form-control" %>
    </div>
  </div>
  <div class="col-md-4">
    <div class="col-md-6">
      <%= f.label :start_aspiration_3 %>
      <%= f.date_field :start_aspiration_3, class: "form-control" %>
    </div>
    <div class="col-md-6">
      <%= f.label :end_aspiration_3 %>
      <%= f.date_field :end_aspiration_3, class: "form-control" %>
    </div>
  </div>
  <br>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>
