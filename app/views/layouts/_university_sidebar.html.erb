      <li>
        <%= link_to recomment_benchmarks_universities_majors_path do %>
          <i class="fa fa-dashboard"></i>
          <span><%= t("university.recomment_benchmarks") %></span>
        <% end %>
      </li>
      <li>
        <%= link_to universities_benchmarks_path do %>
          <i class="fa fa-dashboard"></i>
          <span><%= t("university.manage_benchmarks") %></span>
        <% end %>
      </li>
      <li>
        <%= link_to "" do %>
          <i class="fa fa-dashboard"></i>
          <span><%= t("university.manage_registers_elect") %></span>
        <% end %>
      </li>
