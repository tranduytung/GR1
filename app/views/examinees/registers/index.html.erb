<div class="box box-primary">
  <div class="box-body box-profile">
  <h2 class="text-center">Dang ki NV1</h2>
  <br>
  <%= form_for @current_examinee, url: examinees_registers_path, method: :get do |f| %>
    <table class="table table-bordered table-striped table-condensed">
      <thead>
        <tr>
          <th class="center"><%= "STT" %></th>
          <th class="center"><%= "Major Code" %></th>
          <th class="center"><%= "University" %></th>
          <th class="center"><%= "Target" %></th>
          <th class="center"><%= "Status" %></th>
          <th class="center"><%= "Group Exam" %></th>
          <th class="center"><%= "Score" %></th>
          <th class="center"><%= t "product.action" %></th>
        </tr>
      </thead>
      <tbody>
        <%= f.fields_for :registers do |register| %>
          <%= render "register_fields", ff: register %>
        <% end %>
      </tbody>
    </table>
    <div class="text-center">
      <%= f.submit "Submit", class: "btn btn-primary" %>
    </div>
  <% end %>
  </div>
</div>
